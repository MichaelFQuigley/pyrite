func test(iii:(i:String) -> Void, s:String) {
   iii(s)

  let im = (if(true) iii else println)
  im("a")
  let im2 = (if(true) println else iii)
  im2("b")
  let im3 = println
  im3("c")
  let im4 = iii
  im4("d")
  getBoxedFn1()("e")
  getBoxedFn2()("f")

