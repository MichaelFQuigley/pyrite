func map(l:[Int], mapFunc:(i:Int) -> Int) -> [Int] {
  let result:[Int] = []
  for(i in 0..l.size()) {
    result += [mapFunc(l[i])]
  }
  result
}

func mapFn(i:Int) -> Int i*2

func retMapFn() -> (i:Int) -> Int mapFn

func main() {
  let l = [0,1,2,3,4,5,6]
  println(map(l, mapFn).str())
  println(map(l, retMapFn()).str())
}
