func fib(n:Int, cache:[Int]) -> Int
        if( n <= 1 ) 1
        else if( cache[n] != 0 ) cache[n]
        else
        {
            cache[n] = fib(n - 1, cache) + fib(n - 2, cache)
            cache[n]
        }

func main()
{
    let maxN  = 85
    let cache = [1, 1]

    for( i in 2..(maxN+1) )
        cache += 0

    fib(maxN, cache)

    for( i in 0..(maxN+1) )
        println(String_Int(cache[i]))
}
